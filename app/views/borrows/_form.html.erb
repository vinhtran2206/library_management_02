<%= form_for(@borrow) do |f| %>
  <%= render "shared/error_messages", object: @borrow%>

  <%= f.label :note %>
  <%= f.text_field :note, class: "form-control" %>

   <%= f.label :date_borrow %>
  <%= f.date_field :date_borrow, class: "form-control" %>

  <%= f.label :end_date_borrow %>
  <%= f.date_field :end_date_borrow, class: "form-control" %>

  <%= f.submit yield(:button_text), class: "btn btn-primary" %>
<% end %>
